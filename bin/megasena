#!/usr/bin/env node
'use strict';

var num
  , program = require('commander')
  , megasena = require('../megasena.js')

program
  .version(require('../package.json').version)
  .option('-n, --numbers', 'Total of numbers to generate')
  .parse(process.argv)

num = (program.numbers && !isNaN(program.numbers)) ? Number(program.numbers) : 6;
console.log(megasena(num, true))

